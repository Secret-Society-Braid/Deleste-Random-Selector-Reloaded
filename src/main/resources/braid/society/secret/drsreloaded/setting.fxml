<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="top" styleClass="AnchorPane"
  stylesheets="@style/setting.css" xmlns="http://javafx.com/javafx/8">
  <children>
    <BorderPane prefHeight="200.0" prefWidth="200.0" styleClass="BorderPane">
      <bottom>
        <HBox BorderPane.alignment="CENTER" id="bottom-h-box" prefHeight="100.0" prefWidth="200.0"
          styleClass="HBox">
          <children>
            <Button mnemonicParsing="false" text="Button"/>
            <Button mnemonicParsing="false" text="Button"/>
          </children>
        </HBox>
      </bottom>
      <center>
        <HBox BorderPane.alignment="CENTER" id="center-h-box" prefHeight="100.0" prefWidth="200.0"
          styleClass="HBox" stylesheets="@style/setting.css">
          <children>
            <VBox prefHeight="200.0" prefWidth="100.0" styleClass="VBox">
              <children>
                <CheckBox mnemonicParsing="false" text="CheckBox"/>
                <CheckBox mnemonicParsing="false" text="CheckBox"/>
              </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" styleClass="VBox">
              <HBox.margin>
                <Insets/>
              </HBox.margin>
              <children>
                <Label text="Label"/>
                <Label text="Label"/>
              </children>
            </VBox>
          </children>
        </HBox>
      </center>
      <top>
        <Label BorderPane.alignment="CENTER" id="title" text="Setting"/>
      </top>
    </BorderPane>
  </children>
</AnchorPane>
